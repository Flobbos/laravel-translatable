{
    "name": "flobbos/laravel-translatable-db",
    "description": "A Laravel package for multilingual models",
    "keywords": [
        "laravel",
        "translation",
        "language",
        "database"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexej Krzewitzki",
            "email": "alexej@helloo.it",
            "homepage": "http://helloo.it"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": ">=5.5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flobbos\\TranslatableDB\\TranslatableDBServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Flobbos\\TranslatableDB\\": "src/TranslatableDB/"
        },
        "classmap": []
    },
    "minimum-stability": "dev"
}